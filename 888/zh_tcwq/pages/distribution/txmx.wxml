<view class="list_header">
    <view bindtap="tabClick" class="list_title {{activeIndex==index?'default':'selsect'}}" id="{{index}}" wx:for="{{tabs}}">
        <view>{{item}}</view>
        <view class="list_border" hidden="{{activeIndex!=index}}" style="background-color:#ff4544;"></view>
    </view>
</view>
<view class="list" hidden="{{activeIndex!=0}}">
    <view class="list_imgbk2" hidden="{{dsh.length!=0}}">
        <image class="list_imgbk" src="../image/none_comment.png"></image>
        <view class="list_imgbk3">您暂时没有待审核的明细哦！</view>
    </view>
    <view class="weui-cells weui-cells_after-title" wx:for="{{dsh}}">
        <view class="weui-cell">
            <view class="weui-cell__bd">申请时间：{{item.time}}</view>
            <view class="weui-cell__ft" style="color:#999">等待审核</view>
        </view>
        <view class="weui-cell">
            <view class="weui-cell__hd">
                <image class="himg" src="../image/zhifubao.png" wx:if="{{item.type=='1'}}"></image>
                <image class="himg" src="../image/weixin.png" wx:if="{{item.type=='2'}}"></image>
                <image class="himg" src="../image/yhk.png" wx:if="{{item.type=='3'}}"></image>
            </view>
            <view class="weui-cell__bd">
                <view>{{item.user_name}}</view>
                <view>提现金额：{{item.tx_cost}}元</view>
            </view>
            <view class="weui-cell__ft">预计到账金额：{{item.sj_cost}}</view>
        </view>
        <view class="weui-cell">
            <view class="weui-cell__bd" wx:if="{{item.type=='1'}}">支付宝帐号：{{item.account}}</view>
            <view class="weui-cell__bd" wx:if="{{item.type=='2'}}">微信帐号：{{item.account}}</view>
            <view class="weui-cell__bd" wx:if="{{item.type=='3'}}">银行卡号：{{item.account}}</view>
            <navigator openType="redirect" url="../content/content">
                <view class="weui-cell__ft" style="font-size:26rpx; color:#ff4544;border:1rpx solid #ff4544;padding:6rpx 15rpx;border-radius:30rpx;\"">联系客服</view>
            </navigator>
        </view>
    </view>
    <view class="kong" hidden="{{dsh.length==0}}">
        <image src="../image/wushuju.png"></image>
        <view>没有更多了</view>
    </view>
</view>
