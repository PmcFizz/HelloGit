.nav {
    width: 100%;
    height: 87rpx;
    border-top: 1rpx solid #e5e5e5;
    position: fixed;
    bottom: 0;
    left: 0;
    background: #fff;
}

.nav>view {
    width: 25%;
    height: 100%;
    display: inline-block;
    vertical-align: top;
    position: relative;
}

.nav>view view:nth-child(1) {
    width: 100%;
    height: 60rpx;
}

.nav>view view:nth-child(2) {
    width: 100%;
    height: 26rpx;
    font-size: 19rpx;
    line-height: 26rpx;
    text-align: center;
    font-weight: 595;
    position: absolute;
    bottom: 0;
}

.nav>view view image {
    width: 49rpx;
    height: 49rpx;
    bottom: 0;
}

.shun {
    width: 100%;
    overflow-x: hidden;
}

.header {
    width: 100%;
    height: 360rpx;
}

.swiper-container {
    position: relative;
}

.swiper-container .swiper {
    height: 360rpx;
}

.swiper-container .swiper .img {
    width: 100%;
