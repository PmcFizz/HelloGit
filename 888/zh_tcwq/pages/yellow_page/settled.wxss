    border-bottom: 1rpx solid #e5e5e5;
}

.yz_tel view text:nth-child(1) {
    line-height: 80rpx;
}

.yz_tel view button:nth-child(2) {
    padding: 0 15rpx;
    font-size: 24rpx;
    height: auto;
    float: right;
    margin-right: 20rpx;
    margin-top: 10rpx;
    background: #ED414A;
    border-radius: 10rpx;
    color: #fff;
}

.upload_logo>view image {
    width: 140rpx;
    height: 140rpx;
}

