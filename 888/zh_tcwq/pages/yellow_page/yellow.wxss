.nav {
    width: 100%;
    height: 87rpx;
    border-top: 1rpx solid #e5e5e5;
    position: fixed;
    bottom: 0;
    left: 0;
    background: #fff;
}

.nav>view {
    width: 25%;
    height: 100%;
    display: inline-block;
    vertical-align: top;
    position: relative;
}

.nav>view view:nth-child(1) {
    width: 100%;
    height: 60rpx;
}

.nav>view view:nth-child(2) {
    width: 100%;
    height: 26rpx;
    font-size: 19rpx;
    line-height: 26rpx;
    text-align: center;
    font-weight: 595;
    position: absolute;
    bottom: 0;
}

.nav>view view image {
    width: 49rpx;
    height: 49rpx;
    bottom: 0;
}

.home {
    width: 100%;
    overflow-x: hidden;
}

.body {
    width: 100%;
    height: auto;
}

.banner {
    width: 100%;
    height: 360rpx;
}

.loading {
    width: 100%;
    height: 60rpx;
    background: #fff;
    color: #999;
    line-height: 60rpx;
    text-align: center;
    font-size: 22rpx;
}

.search_yellow {
    width: 100%;
    height: auto;
    top: 116rpx;
    z-index: 9000000000;
    background: #fff;
}

.search_yellow>view {
    border-bottom: 1rpx solid #e5e5e5;
    height: 70rpx;
    line-height: 70rpx;
    text-indent: 20rpx;
    font-size: 28rpx;
    color: #999;
}

.loading image {
    width: 69rpx;
    height: 15rpx;
}

.swiper-container {
    position: relative;
}

.swiper-container .swiper {
    height: 360rpx;
}

.swiper-container .swiper .img {
    width: 100%;
    height: 100%;
}

.dots {
    position: absolute;
    left: 0;
    right: 0;
    bottom: 20rpx;
    display: flex;
    width: 238rpx;
    margin: auto;
    border-radius: 10rpx;
    height: 20rpx;
    justify-content: center;
}

.dot {
    margin: 4rpx 8rpx;
    width: 30rpx;
    height: 6rpx;
    background: rgb(255,255,255);
    border: rgb(218,69,70) solid 1rpx;
    border-radius: 8rpx;
    transition: all 0.6s;
}

.active {
    width: 30rpx;
    background: #f80;
}

.searchbox {
    height: 116rpx;
    padding: 0 30rpx;
    background-color: white;
    position: relative;
}

.release {
    position: fixed;
    z-index: 5000;
    background: #fff;
    width: 90rpx;
    height: 90rpx;
    border-radius: 50%;
    right: 50rpx;
    bottom: 150rpx;
}

.release image {
    width: 100%;
    height: 100%;
}

.input {
    font-size: 26rpx;
    color: #666;
    height: 64rpx;
    border: 5rpx solid #ff4847;
    width: 483rpx;
    position: absolute;
    top: 26rpx;
    padding-left: 40rpx;
    border-radius: 37rpx 0 0 37rpx;
}

.search {
    padding: 0;
    width: 158rpx;
    height: 72rpx;
    color: white;
    background-color: #ff4847;
    position: absolute;
    top: 26rpx;
    left: 555rpx;
    line-height: 74rpx;
    border-radius: 0 37rpx 37rpx 0;
}

.ad {
    margin-top: 24rpx;
    height: 64rpx;
    text-align: top;
    background-color: white;
}

.ad-icon {
    line-height: 64rpx;
    margin-top: 15rpx;
    margin-left: 30rpx;
    float: left;
    height: 35rpx;
    width: 40rpx;
}

.adinfo {
    margin-left: 32rpx;
    float: left;
    font-size: 26rpx;
    margin-top: 13rpx;
    width: 650rpx;
}

.classify {
    width: 100%;
    background: #fff;
}

.classify swiper-item {
    width: 100%;
    height: 1005;
}

.classify swiper-item view {
    width: 20%;
    float: left;
}

.classify swiper-item view .classify_logo {
    width: 100%;
    height: 90rpx;
}

.classify swiper-item view .classify_logo image {
    width: 65rpx;
    height: 65rpx;
    background: #fff;
    border-radius: 8rpx;
}

.classify swiper-item view .classify_name {
    width: 100%;
    height: 40rpx;
    line-height: 40rpx;
    font-size: 28rpx;
    color: #666;
}

.join {
    width: 100%;
    height: 90rpx;
    padding: 0 20rpx;
    background-color: white;
    margin-top: 15rpx;
    position: relative;
}

.jion-img {
    display: inline-block;
    width: 79rpx;
    height: 79rpx;
    position: absolute;
    top: 15rpx;
}

.jion-one {
    font-size: 26rpx;
    display: inline-block;
    height: 30rpx;
    line-height: 30rpx;
    position: absolute;
    top: 20rpx;
    left: 133rpx;
}

.jion-two {
    font-size: 26rpx;
    display: inline-block;
    height: 30rpx;
    line-height: 30rpx;
    position: absolute;
    left: 133rpx;
    top: 63rpx;
}

.join-name {
    color: #ed4149;
    font-size: 26rpx;
}

.jiony {
    background-color: #ff6160;
    border-radius: 10rpx;
    width: 158rpx;
    font-size: 26rpx;
    height: 59rpx;
    color: white;
}

.jionbo {
    width: 500rpx;
    height: 35rpx;
    margin-left: 20rpx;
    line-height: 35rpx;
    font-size: 26rpx;
}

.marry {
    width: 100%;
    overflow-x: hidden;
    height: auto;
    background-color: white;
    margin-top: 20rpx;
    font-size: 28rpx;
    border-bottom: 1rpx solid #eee;
    text-align: center;
}

.luntext {
    height: auto;
    display: flex;
    display: -webkit-box;
    flex-direction: column;
}

.luntext_Item {
    padding: 1rpx;
    flex: 1;
    margin-right: 20rpx;
}

.luntext_Item_text {
    font-size: 30rpx;
}

.weui-navbar__item {
    width: 33.3%;
    padding: 15rpx 0;
    position: relative;
}

.weui-navbar__item.weui-bar__item_on {
    color: #eb414a;
    position: relative;
}

.zheline.block {
    display: block;
    background-color: #eb414a;
}

.weui-navbar__slider {
    width: 120rpx;
    height: 3rpx;
    margin-top: 10rpx;
}

.weui-navbar {
    background-color: white;
    border-bottom: none;
}

.weui-tab__content {
    padding-top: 0;
}

.weui-cells {
    margin-top: 20rpx;
}

.zheline {
    background-color: #eb414a;
    height: 5rpx;
    width: 74%;
    position: absolute;
    bottom: 0rpx;
    left: 14%;
    display: none;
}

.yelist {
    margin-bottom: 30rpx;
    background: #fff;
    margin-top: 20rpx;
    padding-bottom: 90rpx;
}

.yellow_list {
    width: 95%;
    padding: 20rpx 0;
    margin: 0 auto 0;
    border-bottom: 1rpx solid #e5e5e5;
}

.list_head {
    width: 100%;
    height: 150rpx;
}

.list_head>view {
    float: left;
    font-size: 26rpx;
    height: 100%;
}

.list_head>view:nth-child(1) {
    width: 20%;
}

.yellow_logo {
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
    left: 0;
}

.yellow_tel {
    width: 50rpx;
    height: 50rpx;
    border-radius: 50%;
}

.list {
    width: 50%;
}

.list>view image {
    width: 30rpx;
    height: 30rpx;
    float: left;
}

.list>view text {
    float: left;
    line-height: 30rpx;
    margin-left: 10rpx;
}

.list>view:nth-child(1) {
    font-size: 30rpx;
    line-height: 90rpx;
    height: 90rpx;
}

.list>view:nth-child(2) {
    font-size: 24rpx;
    height: 30rpx;
    line-height: 0rpx;
    vertical-align: top;
}

.list_head>view:nth-child(3) {
    width: 30%;
}

.tel_icon>view {
    width: 90rpx;
    height: 90rpx;
    right: 15%;
    background: #ED414A;
    border-radius: 50%;
}

.list_body {
    width: 100%;
    height: 50rpx;
    font-size: 26rpx;
    line-height: 40rpx;
}

.list_body>image {
    width: 30rpx;
    height: 35rpx;
    margin-right: 15rpx;
}

.list_body>text {
    vertical-align: top;
    display: inline-block;
}

.list_body>text:nth-child(3) {
    float: right;
    color: #ED414A;
    margin-right: 4%;
}

.list_name view {
    display: inline-block;
    vertical-align: top;
}

.list_name view image {
    width: 40rpx;
    height: 40rpx;
    right: 0;
}

.list_name>view:nth-child(2) {
    width: 50rpx;
    height: 100%;
}

.list_name>view:nth-child(2) {
    max-width: 370rpx;
    height: 100%;
}

.refresh {
    width: 100%;
    height: 40rpx;
    line-height: 40rpx;
    font-size: 22rpx;
    text-align: center;
    color: #999;
}